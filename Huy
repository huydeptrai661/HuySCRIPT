	Name = "Andepzai",
	Callback = function()
        repeat wait() until game:IsLoaded() 
loadstring(game:HttpGet("https://raw.githubusercontent.com/AnDepZaiHub/AnDepZaiHubBeta/refs/heads/main/AnDepZaiHubNewUpdated.lua"))()
